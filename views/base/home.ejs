<% include ../partials/header %>

  <div class="masonry">
    <% data.reverse().forEach(function(item) { %>
      <div class="card item">
        <div class="card-image">
          <a href="/fullreview/<%= item._id %>">
            <img src="<%= item.photo %>">
            <span class="rating-card"><%= item.rating.overall %></span>
          </a>
        </div>

        <div class="card-content">
          <h6>
            <%= item.title %>
          </h6>
        </div>
      </div>
      <% }); %>
  </div>
  
  <% include ../partials/footer %>
