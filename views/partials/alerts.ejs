<% if (alerts.error) { %>
  <% alerts.error.forEach(function(msg) { %>
    <div class="alert alert-danger mb-0">
      <div class="container">
        <%= msg %>
      </div>
    </div>
  <% }); %>
<% } %>
<% if (alerts.success) { %>
  <% alerts.success.forEach(function(msg) { %>
    <div class="alert alert-warning mb-0">
      <div class="container">
        <%= msg %>
      </div>
    </div>
  <% }); %>
<% } %>
