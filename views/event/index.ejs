
<div class='container'>
  <% include ../partials/alerts %>
  <div class='row'>
    <div class = 'col-md-12 panel panel-default'>
      <h1> New Event</h1>
      <div class='flex col-md-12'>
    <% results.events.forEach( (event) => { %>
    <a class='flex col-md-4' href='/event/<%=event._id%>'><div class='col-md-10 panel panel-default'>
      <h2><%= event.name%></h2>
      <ul>
        <li>Date: <%= event.startDate%> - <%= event.endDate%></li>
        <li>Venue: <%= event.location%></li>
        <li>Organiser: <%= event.creator.name%></li>
      </ul>
    </div></a>
    <%})%>
  </div>
  </div>
  </div>
</div>
