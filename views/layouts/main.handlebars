    <script>
      var total = {{{voteData}}}
      for (var i =0; i < total.length; i++) {
        if (total[i]._id.month === 1 && total[i]._id.points === 1) var upJan = total[i].count
        if (total[i]._id.month === 2 && total[i]._id.points === 1) var upFeb = total[i].count
        if (total[i]._id.month === 3 && total[i]._id.points === 1) var upMar = total[i].count
