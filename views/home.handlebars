{{#each showlist as |showlist|}}
<div class="row">
        <div class="col s12 m7">
          <div class="card">
            <div class="card-image">
              <img src="https://ucarecdn.com/6b34b77d-cb96-458d-92fe-2e8b9339db87/11782268_691831654254741_798037914440118603_o.jpg">
              <span class="card-title">{{ showlist.name }}</span>
            </div>
            <div class="card-content">
              <p>Info about show</p>
            </div>
            <div class="card-action">
              <a href="/show/{{showlist.slug}}">Show Available Tours</a>
            </div>
          </div>
        </div>
      </div>
{{/each}}
