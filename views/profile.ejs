<div class="container-fluid jumbotron">
  <h1>Welcome to your Profile Page, <%= currentUser.name %></h1>
</div>

<div class="row container">
  <% items.forEach(function(item) { %>
    <div class="col-sm-6 col-md-6">
    <div class="thumbnail">
    <div class="caption">
    <h3><a href="<%=item.linkforproduct%>"><%= item.productname %></a></h3>
    <h6> Respond By: <%= item.respondby %></h6>
    <p>
    <a href="/products/<%= item._id %>/edit" class="btn btn-default" role="button">Edit</a>
    <a href="/products/<%= item._id %>" class="btn btn-default" role="button">Delete</a>
    </p>
    </div>
    </div>
    </div>
    <% }) %>
  </div>
