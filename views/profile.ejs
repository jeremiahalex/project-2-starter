<h1>welcome back, <%= locals.user.firstName.toLowerCase() %>!</h1>
<h1>these are your playlists.</h1>
<a href="/playlists/create"><button class="btn btn-primary">create a playlist</button></a>
<hr>
<% playlists.forEach(e => { %>
<a href="<%= '/playlists/'+e._id %>">
  <div class="col-md-4 playlist-panel">
    <h1><%= e.name %></h1>
    <hr>
    <p>By <%= e.creator.name %></p>
  </div>
</a>
<% }) %>